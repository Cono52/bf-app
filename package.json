{
  "name": "app",
  "version": "0.1.0",
  "homepage": "https://cono52.github.io/bf-app",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.1.10",
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.1.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "lodash": "^4.17.10",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1",
    "react-fontawesome": "^1.6.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "styled-components": "^3.3.3"
  },
  "scripts": {
    "start": "cross-env REACT_APP_STAGE=dev react-scripts start",
    "lint": "./node_modules/.bin/eslint ./src",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "cross-env REACT_APP_STAGE=production react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "chrome": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --remote-debugging-port=9222 'http://localhost:3000'"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.12",
    "@types/jest": "^23.3.0",
    "babel-eslint": "^8.2.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.76.0",
    "gh-pages": "^1.2.0",
    "jest-enzyme": "^6.0.2",
    "react-test-renderer": "^16.4.1"
  }
}
