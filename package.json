{
  "name": "app",
  "version": "0.1.0",
  "homepage": "https://cono52.github.io/bf-app",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "lodash": "^4.17.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "styled-components": "^3.2.6"
  },
  "scripts": {
    "start": "cross-env REACT_APP_STAGE=dev react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "cross-env REACT_APP_STAGE=production react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "chrome": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --remote-debugging-port=9222 'http://localhost:3000'"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.72.0",
    "gh-pages": "^1.1.0",
    "jest-enzyme": "^6.0.0",
    "react-test-renderer": "^16.3.2"
  }
}
